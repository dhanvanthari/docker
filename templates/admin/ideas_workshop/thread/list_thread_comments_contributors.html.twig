{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% for adherent in object.getContributors %}
        <a href="{{ path('admin_app_adherent_edit', {id: adherent.id }) }}">
            {{ adherent }}{% if not loop.last %},{% endif %}
        </a>
    {% endfor %}
{% endblock %}
